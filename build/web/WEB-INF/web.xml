<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CompanyReg</servlet-name>
        <servlet-class>Company.CompanyReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newreg</servlet-name>
        <servlet-class>Company.newreg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CompanyLogin</servlet-name>
        <servlet-class>Company.CompanyLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostAnswer</servlet-name>
        <servlet-class>Company.PostAnswer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Company.PostAnswer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CompanyReg</servlet-name>
        <url-pattern>/CompanyReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newreg</servlet-name>
        <url-pattern>/newreg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CompanyLogin</servlet-name>
        <url-pattern>/CompanyLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostAnswer</servlet-name>
        <url-pattern>/PostAnswer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
